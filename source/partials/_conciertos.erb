<div class="conciertos">
  <h2>Concierto de presentación del demo</h2>
  <hr/>
  <% data.conciertos.each do |concierto| %>
    <div class="concert-row">
      <div class="fecha">
        <%= concierto.fecha %>
      </div>
      <div class="descripcion">
        <%= concierto.lugar %><br/>
        <%= concierto.hora %><br/>
        <%= concierto.cover %>
        <br/>
        <a href="<%= concierto.link %>" target="_blank">Más info ></a>
      </div>
    </div>
  <% end %>
</div>
